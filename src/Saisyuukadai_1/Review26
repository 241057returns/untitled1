public class Review26 {
    public static void main(String[] args) {
        int[] numArray = {10, 20, 30, 20, 40, 20};
        int target = 20;
        int count = 0;

        for (int i = 0; i < numArray.length; i++) {
            if (numArray[i] == target) { 
                count++;
            }
        }
        System.out.println("探索した値:" + target);
        System.out.println("一致した数:" + count);
    }
}